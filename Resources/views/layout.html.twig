<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
    <head>
        <title>App manager</title>
        <link rel="stylesheet" href="{{ asset('bundles/elendevappmanager/css/jquery-ui-1.9.2.custom.min.css') }}" />
        <script type="text/javascript" src="{{ asset('bundles/elendevappmanager/js/jquery-1.8.3.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/elendevappmanager/js/jquery-ui-1.9.2.custom.min.js') }}"></script>
        {% block head %}{% endblock head %}
    </head>
    <body>
        
		{% for flashMessage in app.session.flashbag.get('notice') %}
            <div class="flash-notice">
        {{ flashMessage }}
            </div>
        {% endfor %}
		{% for flashMessage in app.session.flashbag.get('error') %}
            <div class="ui-state-error ui-corner-all" style="padding: 0 .7em;">
            <span class="ui-icon ui-icon-alert" style="float: left; margin-right: .3em;"></span>
        {{ app.session.flash('error') }}
            </div>
		{% endfor %}
        
        {% block body %}{% endblock body %}
        
    </body>
    
</html>